    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate:forum": "drizzle-kit generate --config src/plugins/forum/admin/database/drizzle.config.ts",
    "migrate:admin": "drizzle-kit generate --config src/plugins/core/admin/database/drizzle.config.ts",
    "migrate:blog": "drizzle-kit generate --config src/plugins/blog/admin/database/drizzle.config.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
