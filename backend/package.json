    "nodemailer": "^6.9.13",
    "react": "^18.3.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "tar": "^7.2.0",
    "ua-parser-js": "^1.0.38"
  },
