{
  "name": "vitnode-backend",
  "version": "0.0.10-canary.4",
  "description": "Backend wrapper for VitNode projects",
  "author": "VitNode Team",
  "license": "MIT",
  "homepage": "https://vitnode.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitNode/vitnode.git",
    "directory": "packages/backend"
  },
  "bin": {
    "vitnode-backend": "dist/scripts/setup.js"
  },
  "scripts": {
    "build": "tsc && swc src -d dist --config-file .swcrc",
    "build:scripts": "tsup",
    "dev": "concurrently \"tsc -w --preserveWatchOutput\" \"swc src -d dist --config-file .swcrc -w\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    "./main": {
      "require": "./dist/src/main.js",
      "types": "./dist/src/main.d.ts"
    },
    "./providers/*": {
      "require": "./dist/src/providers/*.js",
      "types": "./dist/src/providers/*.d.ts"
    },
    "./utils/*": {
      "require": "./dist/src/utils/*.js",
      "types": "./dist/src/utils/*.d.ts"
    },
    "./app.module": {
      "require": "./dist/src/app.module.js",
      "types": "./dist/src/app.module.d.ts"
    }
  },
  "keywords": [
    "vitnode",
    "nestjs",
    "nest.js",
    "nest",
    "graphql",
    "drizzle",
    "typescript",
    "appollo",
    "serve-static",
    "react-email",
    "swc",
    "sharp",
    "pg",
    "postgresql",
    "express",
    "codegen"
  ],
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/schedule": "^4.1.1",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.39",
    "@types/cookie-parser": "^1.4.7",
    "@types/node": "^22.7.9",
    "@types/pg": "^8.11.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^9.0.1",
    "eslint-config-typescript-vitnode": "workspace:*",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@nestjs/swagger": "^7.4.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.26.2",
    "drizzle-orm": "^0.35.3",
    "helmet": "^8.0.0",
    "pg": "^8.13.0",
    "vitnode-shared": "workspace:*"
  }
}
