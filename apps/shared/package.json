    "@types/node": "^20.3.1",
    "concurrently": "^9.0.1",
    "eslint-config-typescript-vitnode": "workspace:*",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@nestjs/swagger": "^7.4.2"
  }
